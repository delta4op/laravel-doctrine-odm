{
    "name": "delta4op/laravel-mongodb-odm",
    "description": "A lightweight Laravel wrapper around Doctrine's mongodb-odm Document Mapper",
    "homepage": "https://github.com/delta4op/laravel-mongodb-odm",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Delta4op\\MongoODM\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ravish",
            "email": "gitwithravish@gmail.com"
        }
    ],
    "require": {
        "doctrine/mongodb-odm": "2.3.*",
        "illuminate/support": "^6|^7|^8|^9",
        "ext-mongodb": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6|^7|^8|^9",
        "orchestra/testbench": "6.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Delta4op\\MongoODM\\DocumentManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
